<!DOCTYPE html>
<html
  lang="en"
  class="bg-indigo-900 text-white"
  data-version="{{ meta.version }}"
>
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="dns-prefetch" href="https://assets.sembark.com" />
    <link rel="dns-prefetch" href="https://rsms.com" />
    <link rel="assets" href="https://assets.sembark.com" />
    <title>{{ meta.appName }} - {{ seo.title or title }}</title>
    <meta property="twitter:title" content="{{ seo.title or title }}" />
    <meta property="og:title" content="{{ seo.title or title }}" />
    <meta
      name="description"
      content="{{ seo.description or description or  meta.description }}"
    />
    <meta
      property="og:description"
      content="{{ seo.description or description or meta.description }}"
    />
    <meta
      name="twitter:description"
      value="{{ seo.description or description or meta.description }}"
    />
    <meta property="twitter:site" content="{{ meta.twitterUsername }}" />
    <meta property="twitter:creator" content="{{ meta.twitterCreator }}" />
    <meta property="twitter:card" content="summary_large_image" />
    {% if ogImage %} {% set ogImageUrl = meta.publicUrl + "/assets" + ogImage +
    "?v=" + meta.version%} {% else %} {% set ogImageUrl = meta.ogImageUrl %} {%
    endif %}
    <meta property="twitter:image:src" content="{{ ogImageUrl }}" />
    <meta property="og:image:secure_url" content="{{ ogImageUrl }}" />
    <meta property="og:image" content="{{ ogImageUrl }}" />
    <meta property="og:image:type" content="image/png" />
    <meta property="twitter:image:width" content="1200" />
    <meta property="twitter:image:height" content="630" />
    <meta property="og:image:width" content="1200" />
    <meta property="og:image:height" content="630" />
    <meta property="og:site_name" content="{{ meta.appName }}" />
    <meta property="og:url" content="{{ meta.publicUrl }}" />
    <meta property="og:type" content="website" />
    <meta name="keywords" value="{{ meta.keywords }}" />
    <meta name="theme-color" content="#1A202C" />
    <meta name="apple-mobile-web-app-title" content="{{ meta.appName }}" />
    <meta name="application-name" content="{{ meta.appName }}" />

    <link rel="stylesheet" href="/assets/main.css?v={{ meta.version }}" />

    <!-- fluid icon for Mac OS desktop app -->
    <link
      rel="fluid-icon"
      href="https://assets.sembark.com/images/logos/logo_512x512.png"
      title="Sembark"
    />
    <link
      rel="alternate icon"
      class="js-site-favicon"
      type="image/png"
      href="https://assets.sembark.com/favicons/favicon.png"
    />
    <link
      rel="icon"
      class="js-site-favicon"
      type="image/svg+xml"
      href="https://assets.sembark.com/favicons/favicon.svg"
    />
    <link
      rel="mask-icon"
      href="https://assets.sembark.com/favicons/favicon.svg"
      color="#1A202C"
    />
    <!-- the inner fonts -->
    <link rel="stylesheet" href="https://rsms.me/inter/inter.css" />
    <style>
      html {
        scroll-behavior: smooth;
      }
      html,
      body {
        font-family: "Inter var", sans-serif !important;
      }
    </style>
    <script
      src="https://cdn.jsdelivr.net/gh/alpinejs/alpine@v2.8.0/dist/alpine.min.js"
      defer
    ></script>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script
      async
      src="https://www.googletagmanager.com/gtag/js?id={{
        services.googleAnalytics.measurementId
      }}"
    ></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag() {
        let isProduction = "{{ meta.appEnv }}" === "production";
        if (!isProduction) {
          console.log(arguments);
        } else {
          dataLayer.push(arguments);
        }
      }
      gtag("js", new Date());
      gtag("config", "{{ services.googleAnalytics.measurementId }}", {
        cookie_domain: "sembark.com",
        cookie_flags: "SameSite=None;Secure",
      });
      var locationSearch = location.search.substring(1);
      function parseSearch(search) {
        try {
          return JSON.parse(
            '{"' + search.replace(/&/g, '","').replace(/=/g, '":"') + '"}',
            function (key, value) {
              return key === "" ? value : decodeURIComponent(value);
            }
          );
        } catch (e) {
          return {};
        }
      }
      var searchQueryObject = parseSearch(locationSearch);
      if (searchQueryObject.ref) {
        gtag("event", "ref_from_quote_assets", searchQueryObject.ref);
      }
    </script>
  </head>
